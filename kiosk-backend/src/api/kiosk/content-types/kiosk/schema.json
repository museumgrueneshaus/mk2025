{
  "kind": "collectionType",
  "collectionName": "kiosks",
  "info": {
    "singularName": "kiosk",
    "pluralName": "kiosks",
    "displayName": "Kiosk",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "standort": {
      "type": "string"
    },
    "mac_adresse": {
      "type": "string",
      "unique": true
    },
    "ip_adresse": {
      "type": "string"
    },
    "letzter_kontakt": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": ["online", "offline", "wartung"]
    },
    "modus": {
      "type": "enumeration",
      "enum": ["explorer", "slideshow", "website", "videoplayer", "buch"]
    },
    "konfiguration": {
      "type": "dynamiczone",
      "components": [
        "kiosk.explorer-konfiguration",
        "kiosk.slideshow-konfiguration",
        "kiosk.website-konfiguration",
        "kiosk.videoplayer-konfiguration",
        "kiosk.buch-konfiguration"
      ]
    },
    "mediathek": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::mediathek.mediathek",
      "mappedBy": "kiosks"
    }
  }
}